<ivysettings>
    <settings defaultResolver="central"/>
    <resolvers>
        <ibiblio name="central" m2compatible="true"/>
        <filesystem name="local">
            <artifact pattern="${ivy.settings.dir}/libs/[artifact]-[revision].[ext]"/>
        </filesystem>
    </resolvers>
    <modules>
        <module name="powermock-mockito2" resolver="local"/>
    </modules>
</ivysettings>